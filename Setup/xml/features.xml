<?xml version="1.0" encoding="UTF-8"?>
<root>
    <classes>
        <class>
            <name>Features</name>
            <table>dnd_features</table>
            <namespace>DND\Objects</namespace>
            <constants></constants>
            <includes></includes>
            <uses></uses>
            <extends></extends>
            <implements>\DND\Interfaces\Objects</implements>
            <variables>
                <variable id="name">varchar(200)</variable>
                <variable id="description">text</variable>
                <variable id="modifier">varchar(250)</variable>
            </variables>
            <forms>
                <form id="addForm" title="New Feature">
                    <fields>
                        <field label="Name" id="name" var="name" type="text" placeholder="" />
                    </fields>
                    <fields>
                        <field label="Description" id="description" var="description" type="textarea" placeholder="" />
                    </fields>
                    <fields>
                        <field label="Modifier" id="modifier" var="modifier" type="text" placeholder="0" />
                    </fields>
                </form>
                <form id="editForm" title="Edit Feature">
                    <fields>
                        <field label="Name" id="name" var="name" type="text" placeholder="" />
                    </fields>
                    <fields>
                        <field label="Description" id="description" var="description" type="textarea" placeholder="" />
                    </fields>
                    <fields>
                        <field label="Modifier" id="modifier" var="modifier" type="text" placeholder="0" />
                    </fields>
                </form>
            </forms>
            <functions></functions>
            <override></override>
        </class>
    </classes>
</root>
